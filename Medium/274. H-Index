public class Solution {
    public int HIndex(int[] citations) {
        int h = 0;
       Array.Sort(citations, (a, b) => b.CompareTo(a));

        for(int i = 0; i < citations.Length;i++){
            if(citations[i] >= i+1){
                h++;
            }else{
                break;
            }
        }
   return h;
        
    }
}
